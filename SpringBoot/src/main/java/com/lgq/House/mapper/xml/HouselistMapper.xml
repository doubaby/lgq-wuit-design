<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lgq.House.mapper.HouselistMapper">

    <select id="selectBytypeWhole" resultType="com.lgq.House.entity.Houselist">
        select *
        from houselist
        where type = 0 limit 6
    </select>
    <select id="selectBytypeTogether" resultType="com.lgq.House.entity.Houselist">
        select *
        from houselist
        where type = 1 limit 6

    </select>
</mapper>
